{
  "name": "toyar-design",
  "description": "一个用于Vue3.x的ui组件库",
  "main": "./dist/index.js",
  "type": "module",
  "module": "./dist",
  "files": [
    "/dist/*"
  ],
  "keywords": [
    "modules",
    "webUi",
    "toyar"
  ],
  "private": false,
  "version": "0.0.32",
  "scripts": {
    "dev": "vite",
    "build": "vite build -f es",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docsBase",
    "docs:build": "vitepress build docsBase",
    "docs:build1": "node --max_old_space_size=8192 node_modules/vitepress/bin/vitepress.js build docsBase",
    "docs:serve": "vitepress serve docsBase",
    "pub": "npm version patch && npm run build && npm publish && exit 1",
    "pub2": "node ./src/shell/index.js"
  },
  "dependencies": {
    "@vuepress/plugin-register-components": "^2.0.0-rc.15",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "markdown-it-container": "^3.0.0",
    "sass": "^1.69.5",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.12.0",
    "vite": "^4.4.11",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "1.0.0-rc.20",
    "vue-router": "^4.3.0",
    "vue-tsc": "^1.8.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
